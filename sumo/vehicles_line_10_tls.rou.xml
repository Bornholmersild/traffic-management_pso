<?xml version="1.0" encoding="UTF-8"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <!-- Define Vehicle Type -->
    <vType id="car" accel="2.5" decel="4.5" sigma="0.5" length="5" minGap="2.5" maxSpeed="50"/>

    <!-- Define Routes (All Use the Same Edge Sequence) -->
    <route id="route" edges="E1 E2 E3 E4 E5 E6 E7 E8 E9 E10 E11"/>

    <!-- Vehicle Flow (Each Lane Gets Its Own Vehicles) -->
    <flow id="flow_lane0" type="car" route="route" begin="0" end="400" period="2"/>
    
    
</routes>